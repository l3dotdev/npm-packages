{
	"name": "@l3dev/svelte-api",
	"version": "0.2.3",
	"description": "A type-safe result-based API builder for SvelteKit",
	"type": "module",
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"exports": {
		"./types": {
			"types": "./build/types.d.ts"
		},
		"./server": {
			"types": "./build/server/index.d.ts",
			"import": "./build/server/index.js",
			"svelte": "./build/server/index.js"
		},
		"./client": {
			"types": "./build/client/index.d.ts",
			"import": "./build/client/index.js",
			"svelte": "./build/client/index.js"
		}
	},
	"files": [
		"build"
	],
	"scripts": {
		"build": "tsc",
		"package": "pnpm build",
		"prepublishOnly": "pnpm package"
	},
	"author": "l3dotdev",
	"license": "MIT",
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0"
	},
	"dependencies": {
		"@l3dev/api-result": "^0.2.4",
		"@l3dev/result": "^0.3.0",
		"zod": "^3.25.56"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.20.5"
	}
}
